var selected_archive = "<%= escape_javascript(@selected_archive) %>"
//console.log(selected_archive);
if(selected_archive == "graph"){
  $("#archive").html("<%= escape_javascript(render( 'users/graph')) %>");
}
else if(selected_archive == "my_posts"){
  $("#archive").html("<%= escape_javascript(render( 'users/my_posts')) %>");
}
else if(selected_archive == "likes"){
  $("#archive").html("<%= escape_javascript(render( 'users/likes')) %>");
}